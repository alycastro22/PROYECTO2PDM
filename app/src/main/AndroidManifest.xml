<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="hn.edu.ujcv.pdm_2021_i_proyecto2">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.PDM_2021_I_PROYECTO2">
        <activity android:name=".Diseno_Factura_Activity"></activity>
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/Theme.PDM_2021_I_PROYECTO2.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>


        <activity
            android:name=".FacturaActivityListActivity"
            android:label="@string/title_facturaactivity_list"
            android:theme="@style/Theme.PDM_2021_I_PROYECTO2.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".FacturaActivityDetailActivity"
            android:label="@string/title_facturaactivity_detail"
            android:parentActivityName=".FacturaActivityListActivity"
            android:theme="@style/Theme.PDM_2021_I_PROYECTO2.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="hn.edu.ujcv.pdm_2021_i_proyecto2.FacturaActivityListActivity" />
        </activity>
        <activity android:name=".Cliente_Activity" />
        <activity android:name=".Menu_Activity" />
        <activity android:name=".Pedido_Activity" />
        <activity android:name=".Mesa_Activity" />
        <activity android:name=".Empleados_Activity" />


        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>